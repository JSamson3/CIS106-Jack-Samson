            <input variable="Rate"/>
            <output expression="&quot;Weekly &quot; &amp;Hours * Rate" newline="True"/>
            <output expression="&quot;Monthly &quot; &amp;(Hours * Rate)*Weeksinamonth" newline="True"/>
            <output expression="&quot;Yearly &quot; &amp;(Hours * Rate)*Weeksinayear" newline="True"/>
