    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="A simple program that tells you your weekly monthly and yearly wage"/>
            <declare name="Weeksinamonth" type="Real" array="False" size=""/>
            <assign variable="Weeksinamonth" expression="4"/>
            <declare name="Weeksinayear" type="Real" array="False" size=""/>
            <assign variable="Weeksinayear" expression="52"/>
            <declare name="Hours" type="Real" array="False" size=""/>
            <declare name="Rate" type="Real" array="False" size=""/>
